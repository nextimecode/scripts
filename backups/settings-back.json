  "console-ninja.featureSet": "Community",
  "debug.console.fontSize": 13,
  "diffEditor.maxComputationTime": 0,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.accessibilitySupport": "off",
  "editor.codeActionsOnSave": {
    "source.addMissingImports": "explicit",
    "source.fixAll": "explicit",
    "source.fixAll.eslint": "explicit",
    "source.fixAll.format": "explicit"
  },
  "editor.fontFamily": "JetBrains Mono",
  "editor.fontLigatures": true,
  "editor.fontSize": 13,
  "editor.formatOnSave": true,
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.autohide": true,
  "editor.minimap.enabled": false,
  "editor.minimap.maxColumn": 80,
  "editor.parameterHints.enabled": false,
  "editor.renderLineHighlight": "gutter",
  "editor.rulers": [80],
  "editor.scrollBeyondLastColumn": 0,
  "editor.scrollbar.horizontal": "hidden",
  "editor.semanticHighlighting.enabled": false,
  "editor.smoothScrolling": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "editor.wordWrapColumn": 80,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "emmet.syntaxProfiles": {
    "javascript": "jsx"
  },
  "errorLens.scrollbarHackEnabled": true,
  "eslint.codeActionsOnSave.mode": "all",
  "eslint.format.enable": true,
  "eslint.probe": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "graphql"
  ],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "graphql"
  ],
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "*.json": "json",
    "*.tsx": "typescriptreact",
    ".env.*": "dotenv",
    ".prettierrc": "json",
    ".sequelizerc": "javascript",
    ".stylelintrc": "json"
  },
  "files.exclude": {
    "**/*.tsbuildinfo": true,
    "**/.DS_Store": true,
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.idea": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.svn": true,
    "**/CVS": true,
    "**/Thumbs.db": true,
    "apps/*/.cache": true,
    "apps/*/build": true,
    "apps/*/public/build": true
  },
  "files.insertFinalNewline": true,
  "files.watcherExclude": {
    "**/*.log": true,
    "**/.cache/**": true,
    "**/.git/objects/**": true,
    "**/.temp/**": true,
    "**/build/**": true,
    "**/dist/**": true,
    "**/node_modules/**": true,
    "**/out/**": true,
    "**/tmp/**": true
  },
  "git.autofetch": true,
  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "always",
  "github.copilot.advanced": {
    "inlineSuggestCount": 5
  },
  "github.copilot.chat.localeOverride": "pt-br",
  "github.copilot.editor.enableAutoCompletions": true,
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "javascript.suggest.autoImports": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "json.schemas": [],
  "npm.packageManager": "pnpm",
  "prettier.configPath": "prettier.config.js",
  "prisma.scriptRunner": "pnpm",
  "python.venvPath": "~/myenv",
  "scm.inputFontSize": 13,
  "security.workspace.trust.untrustedFiles": "newWindow",
  "svelte.enable-ts-plugin": true,
  "symbols.files.associations": {
    "*.controller.ts": "nest",
    "*.e2e-spec.ts": "ts-test",
    "*.env.example": "gear",
    "*.guard.ts": "typescript",
    "*.module.ts": "nest",
    "*.spec.ts": "ts-test",
    "vitest.config.e2e.ts": "vitest"
  },
  "symbols.hidesExplorerArrows": false,
  "tailwind-fold.autoFold": false,
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1",
    "Q_NEW_SESSION": "1"
  },
  "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.gpuAcceleration": "off",
  "terminal.integrated.profiles.linux": {
    "bash": {
      "icon": "terminal-bash",
      "path": "bash"
    },
    "fish": {
      "path": "fish"
    },
    "pwsh": {
      "icon": "terminal-powershell",
      "path": "pwsh"
    },
    "tmux": {
      "icon": "terminal-tmux",
      "path": "tmux"
    },
    "zsh": {
      "path": "zsh"
    }
  },
  "terminal.integrated.showExitAlert": false,
  "terminal.integrated.tabs.enabled": false,
  "typescript.preferences.importModuleSpecifier": "project-relative",
  "typescript.suggest.autoImports": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.tsserver.log": "off",
  "typescript.updateImportsOnFileMove.enabled": "never",
  "update.mode": "start",
  "window.autoDetectColorScheme": true,
  "window.commandCenter": true,
  "window.nativeTabs": true,
  "window.newWindowDimensions": "fullscreen",
  "window.systemColorTheme": "default",
  "window.titleBarStyle": "native",
  "window.zoomLevel": 0,
  "workbench.activityBar.location": "top",
  "workbench.colorTheme": "poimandres",
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.labelFormat": "short",
  "workbench.externalBrowser": "chrome",
  "workbench.iconTheme": "symbols",
  "workbench.preferredDarkColorTheme": "Dracula Theme",
  "workbench.preferredHighContrastColorTheme": "poimandres",
  "workbench.preferredHighContrastLightColorTheme": "Min Light",
  "workbench.preferredLightColorTheme": "Min Light",
  "workbench.productIconTheme": "fluent-icons",
  "workbench.settings.applyToAllProfiles": [
    "CodeGPT.organizationId",
    "github.copilot.preferredAccount"
  ],
  "workbench.startupEditor": "newUntitledFile"
}
