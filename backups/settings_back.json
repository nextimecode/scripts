  "editor.parameterHints.enabled": false,
  "editor.renderLineHighlight": "gutter",
  "cSpell.language": "en,pt",
  "typescript.updateImportsOnFileMove.enabled": "never",
  "editor.suggestSelection": "first",
  "explorer.confirmDelete": false,
  "terminal.integrated.showExitAlert": false,
  "typescript.suggest.autoImports": true,
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "workbench.editor.labelFormat": "short",
  "editor.fontLigatures": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "cSpell.enableFiletypes": [
    "!asciidoc",
    "!c",
    "!cpp",
    "!csharp",
    "!go",
    "!handlebars",
    "!haskell",
    "!jade",
    "!java",
    "!latex",
    "!php",
    "!pug",
    "!python",
    "!restructuredtext",
    "!rust",
    "!scala",
    "!scss"
  ],

  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "always",
  "git.autofetch": true,

  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,

  "github.copilot.advanced": {
    "inlineSuggestCount": 10
  },

  "editor.acceptSuggestionOnCommitCharacter": false,
  "explorer.compactFolders": false,
  "editor.accessibilitySupport": "off",
  "explorer.confirmDragAndDrop": false,
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font",
  "eslint.format.enable": true,
  "eslint.codeActionsOnSave.mode": "all",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.fixAll": "explicit",
    "source.fixAll.format": "explicit",
    "source.addMissingImports": "explicit"
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "graphql"
  ],
  "eslint.probe": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "graphql"
  ],
  "npm.packageManager": "pnpm",
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnSave": true,
  "prettier.configPath": "prettier.config.js",
  "editor.semanticHighlighting.enabled": false,
  "editor.fontFamily": "JetBrains Mono",
  "editor.tabSize": 2,
  "security.workspace.trust.untrustedFiles": "newWindow",

  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.idea": true,
    "**/.project": true,
    "**/.settings": true,
    "**/*.tsbuildinfo": true,
    "apps/*/.cache": true,
    "apps/*/build": true,
    "apps/*/public/build": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/node_modules/**": true,
    "**/dist/**": true,
    "**/build/**": true,
    "**/out/**": true,
    "**/.cache/**": true,
    "**/tmp/**": true,
    "**/.temp/**": true,
    "**/*.log": true
  },

  "workbench.iconTheme": "symbols",
  "update.mode": "start",
  "terminal.integrated.gpuAcceleration": "off",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "symbols.hidesExplorerArrows": false,
  "workbench.productIconTheme": "fluent-icons",
  "window.commandCenter": true,
  "editor.wordWrap": "wordWrapColumn",
  "editor.inlineSuggest.enabled": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.preferences.importModuleSpecifier": "project-relative",
  "files.insertFinalNewline": true,
  "editor.scrollbar.horizontal": "hidden",
  "errorLens.scrollbarHackEnabled": true,
  "workbench.settings.applyToAllProfiles": ["CodeGPT.organizationId"],
  "editor.minimap.autohide": true,
  "editor.minimap.maxColumn": 80,
  "window.newWindowDimensions": "fullscreen",
  "window.titleBarStyle": "native",
  "window.nativeTabs": true,
  "workbench.colorCustomizations": {
    // "editor.background": "#282c34",
    "sideBar.background": "#19171d",
    "activityBar.background": "#19171d",
    "activityBarBadge.background": "#FF453A"
    // "terminal.background": "#1b1d21"
  },
  "window.autoDetectColorScheme": true,
  "workbench.editor.empty.hint": "hidden",
  "console-ninja.featureSet": "Community",
  "editor.minimap.enabled": false,
  "terminal.integrated.profiles.linux": {
    "bash": {
      "path": "bash",
      "icon": "terminal-bash"
    },
    "zsh": {
      "path": "zsh"
    },
    "fish": {
      "path": "fish"
    },
    "tmux": {
      "path": "tmux",
      "icon": "terminal-tmux"
    },
    "pwsh": {
      "path": "pwsh",
      "icon": "terminal-powershell"
    }
  },
  "editor.smoothScrolling": true,
  "editor.fontSize": 14
}
